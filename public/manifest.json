
{
  "name": "AskGPT",
  "description": "Web page query tool based on langchain and ChatGPT",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "activeTab", "tabs"
 ],
 "commands": {
  "_execute_action": {
    "suggested_key": {
      "default": "Ctrl+Q",
      "mac": "Command+Q"
    }
  }
}
}

  